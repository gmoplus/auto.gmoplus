    <emails version="2.1.0">
        <email version="2.1.0" key="banners_admin_banner_added" subject="A banner added at {site_name}">
        <![CDATA[
A user added a new banner at {site_name}
<b><u>Details</u></b>
Owner: <b>{username}</b>,
Creation Date: <b>{date}</b>,
Status: <b>{status}</b>,
Link: {link}]]>
        </email>
        <email version="2.6.1" key="banners_banner_activated" subject="Your banner enabled at {site_name}">
        <![CDATA[
Hello {name},

Your banner at {site_name} has been enabled.

______________________________
Thank you,
{site_name}
Administration Team
        ]]>
        </email>
        <email version="2.6.1" key="banners_banner_deactivated" subject="Your banner disabled at {site_name}">
        <![CDATA[
Hello {name},

The Administrator has disabled your banner at {site_name}.
Please contact the Administrator to resolve the issue.
{site_email}

______________________________
Thank you,
{site_name}
Administration Team
        ]]>
        </email>
        <email version="2.7.0" key="banners_cron_banner_expired" subject="Your banner expired at {site_name}">
        <![CDATA[
Hello {name},

Your {banner} banner at {site_name} has expired.

______________________________
Thank you,
{site_name}
Administration Team
        ]]>
        </email>
        <email version="2.1.0" key="banners_admin_banner_edited" subject="Banner edited at {site_name}">
        <![CDATA[
The banner at {site_name} has been edited
<b><u>Details</u></b>
Owner: <b>{username}</b>,
Creation Date: <b>{date}</b>,
Status: <b>{status}</b>,
Link: {link}
        ]]>
        </email>
    </emails>

